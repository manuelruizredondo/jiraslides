<script type="application/ld+json">
  {
    "@context": "http://schema.org",
    "@type": "BlogPosting",
    "headline": "{{ title }}",
    "image": [
      "https://{{ global.url }}{{ image }}"
    ],
    "author": {
      "@type": "Person",
      "name": "check pay"
    },
    "datePublished": "{{ date }}",
    "mainEntityOfPage": {
      "@type": "WebPage",
      "@id": "https://{{ global.url }}/blog"
    },
    "publisher": {
      "@type": "Organization",
      "name": "duttijiras",
      "logo": {
        "@type": "ImageObject",
        "url": "https://duttijiras.com/assets/static/favicons/favicon_192x192.png"
